{
  "name": "flizpay/flizpay-for-shopware",
  "description": "FLIZpay payment gateway for Shopware",
  "version": "1.0.0",
  "type": "shopware-platform-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "FLIZpay GmbH"
    }
  ],
  "require": {
    "shopware/core": "^6.7"
  },
  "autoload": {
    "psr-4": {
      "FLIZpay\\FlizpayForShopware\\": "src/"
    }
  },
  "extra": {
    "shopware-plugin-class": "FLIZpay\\FlizpayForShopware\\FlizpayForShopware",
    "label": {
      "de-DE": "FLIZpay Zahlungsgateway",
      "en-GB": "FLIZpay Payment Gateway"
    },
    "description": {
      "de-DE": "Akzeptiere Zahlungen mit FLIZpay in deinem Shopware-Shop. Einfache Integration, sichere Transaktionen und Cashback-Belohnungen fuer alle deine Kunden.",
      "en-GB": "Accept payments with FLIZpay in your Shopware store. Simple and quick integration, secure payment transactions, and automatic cashback rewards for customers."
    },
    "manufacturerLink": {
      "de-DE": "https://flizpay.de",
      "en-GB": "https://flizpay.de"
    },
    "supportLink": {
      "de-DE": "mailto:info@flizpay.de",
      "en-GB": "mailto:info@flizpay.de"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
